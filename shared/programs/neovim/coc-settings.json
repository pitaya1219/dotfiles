{
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.jumpCommand": "tabedit",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/clone/dotfiles/.vim/snippets"
  ],
  "suggest.completionItemKindLabels": {
    "method": "",
    "function": "",
    "variable": "[]",
    "field": "",
    "typeParameter": "",
    "constant": "<>",
    "class": "",
    "interface": "",
    "struct": "",
    "event": "",
    "operator": "",
    "module": "",
    "property": "",
    "enum": "",
    "reference": "",
    "keyword": "",
    "file": "",
    "folder": "",
    "color": "",
    "unit": "",
    "snippet": "",
    "text": "",
    "constructor": "",
    "value": "☆",
    "enumMember": " "
  },
  "languageserver": {
    "nix": {
      "command": "nixd",
      "filetypes": ["nix"],
      "rootPatterns": ["flake.nix", "default.nix"]
    }
  },
  "explorer.focus": true,
  "explorer.file.reveal.auto": false,
  "explorer.toggle": false,
  "explorer.position": "left",
  "explorer.width": 40,
  "explorer.icon.enableNerdfont": true
}
